{
 "scenarios": {
  "simple": {
   "requests": [
    {
     "url": "http://blazedemo.com/", 
     "label": "IndexPage"
    }, 
    {
     "url": "http://blazedemo.com/checkout.php", 
     "label": "CheckoutPage"
    }
   ]
  }
 }, 
 "services": [
  {
